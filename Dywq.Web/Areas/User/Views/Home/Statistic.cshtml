@model Dywq.Web.Dto.Commpany.AdminStatisticInfoDto
@section head{
    <link href="~/css/pager.css" rel="stylesheet" />
    <style type="text/css">
        .todo {
            width: 100%;
            min-height: 30px;
            overflow: hidden;
        }

        .title {
            height: 30px;
            margin-bottom: 20px;
        }

        .items {
            width: 100%;
            min-height: 60px;
        }

            .items ul {
                width: 100%;
                min-height: 60%;
            }

                .items ul li {
                    width: 150px;
                    min-height: 40px;
                    text-align: center;
                    float: left;
                    background-color: white;
                    margin-right: 15px;
                    padding: 15px;
                    margin-bottom: 15px;
                }

                    .items ul li span {
                        width: 100%;
                        text-align: center;
                        float: left;
                    }

                        .items ul li span.number {
                            font-size: 18px;
                            font-weight: bolder;
                        }

        .todo-message {
            width: 100%;
            min-height: 100px;
            overflow: hidden;
        }

            .todo-message ul {
                width: 100%;
                overflow: hidden;
                background-color: white;
                padding: 15px;
                margin-bottom: 0;
            }

                .todo-message ul li {
                    width: 100%;
                    height: 30px;
                    float: left;
                }

                    .todo-message ul li span {
                        float: left;
                    }

                        .todo-message ul li span.cont {
                            width: 80%;
                        }

                        .todo-message ul li span.time {
                            width: 20%;
                            text-align: right;
                            padding-right: 15px;
                        }

        .paginabox {
            background-color: white;
            margin: 0;
            display: block;
            text-align: right;
            padding-right: 20px;
            padding-bottom: 20px;
        }

        .todo-message ul li a {
            color: black;
        }

        .items a {
            color: black;
        }
    </style>
}

<div class="todo">
    <div class="title"><h3>待审核事项</h3></div>
    <div class="items">
        <ul>
            <li>
                <a href="/user/company/companynewslist">
                    <span class="number">@(Model.CompanyInfoCount)</span>
                    <span>企业信息-待审核</span>
                </a>
            </li>
            <li>
                <a href="/user/cooperation/list">
                    <span class="number">@(Model.CooperationInfoCount)</span>
                    <span>企业对接-待审核</span>
                </a>
            </li>
            <li>
                <a href="/user/financing/list">
                    <span class="number">@(Model.FinancingCount)</span>
                    <span>企业融资-待审核</span>
                </a>

            </li>
            <li>
                <a href="/user/purchase/list?type=0">
                    <span class="number">@(Model.PurchaseCount0)</span>
                    <span>企业采购-待审核</span>
                </a>

            </li>
            <li>
                <a href="/user/purchase/list?type=1">
                    <span class="number">@(Model.PurchaseCount1)</span>
                    <span>企业生产-待审核</span>
                </a>
            </li>

            <li>
                <a href="/user/company/list">
                    <span class="number">@(Model.CompanyCount)</span>
                    <span>
                        企业名录-待审核
                    </span>
                </a>
            </li>


            <li>
                <a href="/user/news/list">
                    <span class="number">@(Model.NewsCount)</span>
                    <span>
                        通知公告-待审核
                    </span>
                </a>
            </li>


            <li>
                <a href="/user/investment/list">
                    <span class="number">@(Model.InvestmentCount)</span>
                    <span>
                        招建安环-待审核
                    </span>
                </a>
            </li>

            <li>
                <a href="/user/article/PartyBuildingArticleList">
                    <span class="number">@(Model.PartyBuildingArticleCount)</span>
                    <span>
                        企业党建-待审核
                    </span>
                </a>
            </li>


            <li>
                <a href="/user/article/PolicyArticleList">
                    <span class="number">@(Model.PolicyArticleCount)</span>
                    <span>
                        惠企政策-待审核
                    </span>
                </a>
            </li>


            <li>
                <a href="/user/expert/list">
                    <span class="number">@(Model.ExpertCount)</span>
                    <span>
                        专家库-待审核
                    </span>
                </a>
            </li>

            <li>
                <a href="/user/aboutus/list">
                    <span class="number">@(Model.AboutusCount)</span>
                    <span>
                        关于我们-待审核
                    </span>
                </a>
            </li>



        </ul>
     
    </div>

</div>



<div class="todo">
    <div class="title">
        <h3>统计信息  <span style="font-size:20px;margin-left:10px;"><a style="color:black;text-decoration:none;" href="/user/home/StatisticInfo">按日期查询</a></span></h3>
       
    </div>
    <div class="items">
        <ul>
            <li>
                <a href="/user/article/policyArticleList">
                    <span class="number">@(Model.PolicyArticleTotalCount)</span>
                    <span>惠企政策总数</span>
                </a>
            </li>
            <li>
                <a href="/user/cooperation/list">
                    <span class="number">@(Model.CooperationInfoTotalCount)</span>
                    <span>企业对接信息总数</span>
                </a>
            </li>


        </ul>
    </div>
</div>



<div class="todo-message">


</div>




@section js{
    <script type="text/javascript">

    </script>
}
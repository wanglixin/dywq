@model Dywq.Web.Dto.Article.PartyBuildingArticleDTO
@section head{
    <link href="~/css/index8.css" rel="stylesheet" />

}

<div id="main_content">
    <div class="container">
        <div class="main_title">
            <span class="home">首页</span>/<span class="activething">企业党建</span>
        </div>

        <div style="text-align:center;"><h3>@(Model.ThemeTitle)</h3></div>
        <div style="text-align:center; margin-top:20px;">@(Model.CreatedTime)</div>
        <hr />
        <div style="line-height:2;font-size:16px; text-align:center;">
            @(Html.Raw(Model.Subtitle))
        </div>
        <div style="line-height:2;font-size:16px; text-align:center;">
            <img  style="max-width:80%;" src="@(Model.Pic)"/>
        </div>
        <div style="line-height:2;font-size:16px;">
            @(Html.Raw(Model.Content))
        </div>
    </div>
</div>



